    "@mantine/notifications": "^5.10.2",
    "@tanstack/react-query": "^4.22.4",
    "@tanstack/react-query-devtools": "^4.22.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.7.0",
    "zod": "^3.20.2",
    "zustand": "^4.3.2"
